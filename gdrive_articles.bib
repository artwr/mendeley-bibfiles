month = apr,
number = {4},
pages = {716--29},
pmid = {18022295},
title = {{Modelling radioiodine transport across a capillary fringe.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18022295},
volume = {99},
year = {2008}
}

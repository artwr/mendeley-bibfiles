@article{Vandenhove2007,
abstract = {The present study aimed to quantify the influence of soil parameters on uranium uptake by ryegrass. Ryegrass was established on eighteen distinct soils, spiked with (238)U. Uranium soil-to-plant transfer factors (TF) ranged from 0.0003 to 0.0340kgkg(-1). There was no significant relation between the U soil-to-plant transfer (or total U uptake or flux) and the uranium concentration in the soil solution or any other soil factor measured, nor with the U recovered following selective soil extractions. Multiple linear regression analysis resulted in a significant though complex model explaining up to 99\% of variation in TF. The influence of uranium speciation on uranium uptake observed was featured: UO(2)(+2), uranyl carbonate complexes and UO(2)PO(4)(-) seem the U species being preferentially taken up by the roots and transferred to the shoots. Improved correlations were obtained when relating the uranium TF with the summed soil solution concentrations of mentioned uranium species.},
author = {Vandenhove, H and {Van Hees}, M and Wannijn, J and Wouters, K and Wang, L},
doi = {10.1016/j.envpol.2006.04.012},
issn = {0269-7491},
journal = {Environmental pollution (Barking, Essex : 1987)},
keywords = {Biological Availability,Calcium,Calcium: analysis,Hydrogen-Ion Concentration,Lolium,Lolium: chemistry,Magnesium,Magnesium: analysis,Models, Biological,Potassium,Potassium: analysis,Soil,Soil Pollutants, Radioactive,Soil Pollutants, Radioactive: analysis,Soil Pollutants, Radioactive: pharmacokinetics,Soil: analysis,Uranium,Uranium: analysis,Uranium: pharmacokinetics},
month = jan,
number = {2},
pages = {577--86},
pmid = {16781804},
title = {{Can we predict uranium bioavailability based on soil parameters? Part 2: soil solution uranium concentration is not a good bioavailability index.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16781804},
volume = {145},
year = {2007}

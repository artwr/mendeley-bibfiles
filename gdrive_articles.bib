keywords = {caesium,cobalt,fluxes,plant uptake,radium},
month = jan,
number = {1},
pages = {93--103},
pmid = {15093334},
title = {{Plant uptake of radionuclides in lysimeter experiments.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15093334},
volume = {99},
year = {1998}
}

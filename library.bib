@article{Falta2005,
abstract = {Analytical solutions are developed for approximating the time-dependent contaminant discharge from DNAPL source zones undergoing dissolution and other decay processes. The source functions assume a power relationship between source mass and chemical discharge and can consider partial DNAPL source remediation (depletion) at any time after the initial DNAPL release. The source functions are used as a time-dependent boundary condition in an idealized chemical transport model to develop leading order approximations of the plume response to DNAPL source removal. The results suggest that partial DNAPL remediation does not tend to have a dramatic impact on the maximum extent of the plume if very low concentration values are used to define the plume boundaries. However, the solutions show that partial DNAPL removal from the source zone is likely to lead to large reductions in plume concentrations and mass, and it reduces the longevity of the plume. When the mass discharge from the source zone is linearly related to the DNAPL mass, it is shown that partial DNAPL depletion leads to linearly proportional reductions in the plume mass and concentrations.},
author = {Falta, Ronald W and {Suresh Rao}, P and Basu, Nandita},
doi = {10.1016/j.jconhyd.2005.05.010},
issn = {0169-7722},
journal = {Journal of contaminant hydrology},
keywords = {Models, Chemical,Soil Pollutants,Soil Pollutants: isolation \& purification,Time Factors,Waste Management,Waste Management: methods,Water Movements,Water Pollutants, Chemical,Water Pollutants, Chemical: isolation \& purificati},
month = aug,
number = {4},
pages = {259--80},
pmid = {16019108},
title = {{Assessing the impacts of partial mass depletion in DNAPL source zones I. Analytical modeling of source strength functions and plume response.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16019108},
volume = {78},
year = {2005}
}

doi = {10.1016/j.jhazmat.2008.07.102},
isbn = {1574771450},
issn = {1873-3336},
journal = {Journal of hazardous materials},
keywords = {Ethane,Iron,Iron: chemistry,Kinetics,Trichloroethanes,Trichloroethanes: chemistry},
month = apr,
number = {2-3},
pages = {1315--21},
pmid = {18804328},
title = {{Kinetics of transformation of 1,1,1-trichloroethane by Fe(II) in cement slurries.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18804328},
volume = {163},
year = {2009}
}

journal = {Science (New York, N.Y.)},
keywords = {Atmosphere,Chlorides,Chlorides: analysis,Chlorides: metabolism,Desert Climate,Ecosystem,Nitrates,Nitrates: analysis,Nitrates: metabolism,Plant Roots,Plant Roots: metabolism,Plants,Plants: metabolism,Soil,Soil: analysis,Southwestern United States,Water,Water: chemistry},
month = nov,

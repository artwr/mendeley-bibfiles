keywords = {Bioavailability,Biodegradation,Cometabolism,Mass transfer,Organic contaminants,Sorption},
month = jan,
number = {3-4},
pages = {255--292},
title = {{Modeling and interpreting bioavailability of organic contaminant mixtures in subsurface environments}},

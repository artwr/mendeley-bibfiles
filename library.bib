@book{Benson2006b,
abstract = {The key role of a forensic scientist is to assist in determining whether a crime has been committed, and if so, assist in the identification of the offender. Many people hold the belief that a particular item can be conclusively linked to a specific person, place or object. Unfortunately, this is often not achievable in forensic science. In performing their role, scientists develop and test hypotheses. The significance of those hypotheses that cannot be rejected upon completion of all available examinations/analyses is then evaluated. Although one can generally identify the substances present using available techniques, it is generally not possible to distinguish one source of the same substance from another. In such circumstances, although a particular hypothesis cannot be rejected, it cannot be conclusively proven, i.e. the samples could still have originated from different sources. This limitation of not being able to distinguish between sources currently extends to the analysis of other forensic samples including, but not limited to, ignitable liquids, paints, adhesives, textile fibres, plastics, and illicit drugs. Stable isotope ratio mass spectrometry (IRMS) is an additional technique that can be utilised to test a given hypothesis. This technique shows the potential to be able to individualise a range of materials of forensic interest. This paper provides a brief description of the technique, followed by a review of the various applications of IRMS in different scientific fields. The focus of this summary is on forensic applications of IRMS, in particular the analysis of explosives, ignitable liquids and illicit drugs.},
author = {Benson, Sarah and Lennard, Chris and Maynard, Philip and Roux, Claude},
booktitle = {Forensic science international},
doi = {10.1016/j.forsciint.2005.03.012},
isbn = {0849315220},
issn = {0379-0738},
keywords = {Animals,Archaeology,Doping in Sports,Environmental Pollutants,Environmental Pollutants: analysis,Explosions,Food Analysis,Forensic Medicine,Humans,Isotopes,Isotopes: analysis,Mass Spectrometry,Mass Spectrometry: instrumentation,Mass Spectrometry: methods,Pharmaceutical Preparations,Pharmaceutical Preparations: chemistry,Plant Physiological Phenomena,Quality Control,Species Specificity,Substance Abuse Detection,Substance Abuse Detection: methods},
month = feb,
number = {1},
pages = {1--22},
pmid = {15919168},
title = {{Forensic applications of isotope ratio mass spectrometry--a review.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15919168},
volume = {157},
year = {2006}
}

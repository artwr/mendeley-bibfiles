month = jan,
number = {1},
pages = {10--22},
pmid = {17399863},
title = {{Radiochlorine concentration ratios for agricultural plants in various soil conditions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17399863},
volume = {95},
year = {2007}
}

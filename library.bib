@article{Kashparov2005a,
abstract = {Long-term controlled experiments under natural conditions in the field have been carried out in the Chernobyl Exclusion zone in order to determine the parameters governing radioiodine transfer to plants from four types of soils (podzoluvisol, greyzem and typical and meadow chernozem) homogeneously contaminated in the 20-cm upper layer with an addition of (125)I. An absence of (125)I depletion in arable soil layers due to volatilization was noted up to one year after contamination. During one year, depletion due to the vertical migration of radioiodine from the arable layer of each of the soils did not exceed 4\% of the total (125)I content. Radioiodine concentration ratios (CR) were obtained in radish roots, lettuce leaves, bean pods, and wheat grain and straw. The highest CR values were observed in podzoluvisol: 0.01-0.03 for radish roots and lettuce leaves, 0.003-0.004 for bean pods and 0.001 for wheat grains. In the other three soils, these values were one order of magnitude lower. The parameters relating to changes in radioiodine bioavailability were determined, based on the contamination dynamics of plants in field conditions.},
author = {Kashparov, V and Colle, C and Zvarich, S and Yoschenko, V and Levchuk, S and Lundin, S},
doi = {10.1016/j.jenvrad.2004.06.005},
issn = {0265-931X},
journal = {Journal of environmental radioactivity},
keywords = {Biological Availability,Crops, Agricultural,Crops, Agricultural: chemistry,Crops, Agricultural: metabolism,Food Contamination,Food Contamination: analysis,Iodine Radioisotopes,Iodine Radioisotopes: analysis,Plant Leaves,Plant Leaves: chemistry,Plant Leaves: metabolism,Plant Roots,Plant Roots: metabolism,Plants,Plants: chemistry,Plants: metabolism,Soil,Soil Pollutants, Radioactive,Soil Pollutants, Radioactive: analysis,Soil: analysis,Time Factors,Volatilization},
month = jan,
number = {2},
pages = {187--204},
pmid = {15603907},
title = {{Soil-to-plant halogens transfer studies 1. Root uptake of radioiodine by plants.}},

@techreport{Thompson1992,
abstract = {During second quarter 1992, tritium, gross alpha, total alpha-emitting radium (radium-224 and radium-226), nitrate, nonvolatile beta, lead, cadmium, mercury, nickel, radium-228, trichloroethylene, and tetrachloroethylene exceeded the U.S. Environmental Protection Agency Primary Drinking Water Standards (PDWS) in one or more groundwater samples from monitoring wells at the F-Area Seepage Basins (FASB). Tritium and nitrate were the most widespread constituents; 56 (67\%) of the 84 monitored wells exhibited tritium activities and 41 (49\%) wells contained nitrate concentrations that exceeded the PDWS. These two constituents were found primarily in Aquifer Zone IIB2 (Water Table) and Aquifer Zone IIB 1(Barnwell/McBean). Tritium activities exceeding the PDWS occurred in 8 (44\%) of the 18 Aquifer Unit IIA (Congaree) wells. The highest tritium activity and the highest nitrate concentration were found in Aquifer Zone IIB2 (Water Table). One or more of 49 downgradient Aquifer Zone IIB2 (Water Table) and Aquifer Zone IIB1 (Barnwell/McBean) wells contained levels of tritium, gross alpha, total alpha-emitting radium, nitrate, nonvolatile beta, lead, cadmium, mercury, nickel, trichloroethylene, or tetrachloroethylene that exceeded the PDWS. Upgradient wells FSB 76C and 108D did not contain elevated levels of any of these constituents; however, upgradient wells FSB 99D and 109D exhibited tritium activities that exceeded the PDWS. Upgradient well FSB 99D also contained elevated levels of cadmium. Six downgradient wells in the upper portion of Aquifer Unit IIA (Congaree) contained elevated levels of tritium. Elevated tritium was not found in upgradient wells FSB 76B, 96AR, and 98AR; however, upgradient well FSB 78B exhibited elevated tritium, and upgradient well FSB 79B contained elevated radium-228. No constituents exceeded the PDWS in wells in the lower portion of Aquifer Unit IIA (Congaree).},
address = {Aiken, SC 29808},
author = {Thompson, C. Y.},
institution = {Westinghouse Savannah River Company, Savannah River Site},
month = sep,
title = {{SECOND QUARTER 1992 F-AREA SEEPAGE BASINS GROUNDWATER MONITORING REPORT ( U )}},
year = {1992}
}

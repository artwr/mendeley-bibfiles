month = jun,
number = {12},
pages = {4586--93},
pmid = {16047796},
title = {{Trace analysis of bromate, chlorate, iodate, and perchlorate in natural and bottled waters.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16047796},

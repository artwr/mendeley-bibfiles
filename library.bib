@article{Tartakovsky2011,
abstract = {We consider advective-reactive solute transport in porous media whose hydraulic and transport properties are uncertain. These properties are treated as random fields, which renders nonlinear advection-reaction transport equations stochastic. We derive a deterministic equation for the probability density function (PDF) of the concentration of a solute that undergoes heterogeneous reactions, e.g., precipitation or dissolution. The derivation treats exactly (without linearization) a reactive term in the transport equation which accounts for uncertainty (randomness) in both flow velocity and kinetic rate constants but requires a closure, such as a Large-Eddy-Diffusivity (LED) approximation used in the present analysis. No closure is required when reaction rates are the only source of uncertainty. We use exact concentration PDFs obtained for this setting to analyze the accuracy of our general, LED-based PDF equations.},
author = {Tartakovsky, Daniel M and Broyda, Svetlana},
doi = {10.1016/j.jconhyd.2010.08.009},
issn = {1873-6009},
journal = {Journal of contaminant hydrology},
keywords = {Hydrodynamics,Models,Probability,Soil Pollutants,Soil Pollutants: chemistry,Solutions,Solutions: chemistry,Theoretical},
month = mar,
pages = {129--40},
pmid = {20926156},
title = {{PDF equations for advective-reactive transport in heterogeneous porous media with uncertain properties.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20926156},
volume = {120-121},
year = {2011}
}

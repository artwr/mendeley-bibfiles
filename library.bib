@article{Pang2006,
abstract = {This paper addresses the problem of estimating a likelihood map for the location of the source of a chemical plume using an autonomous vehicle as a sensor probe in a fluid flow. The fluid flow is assumed to have a high Reynolds number. Therefore, the dispersion of the chemical is dominated by turbulence, resulting in an intermittent chemical signal. The vehicle is capable of detecting above-threshold chemical concentration and sensing the fluid flow velocity at the vehicle location. This paper reviews instances of biological plume tracing and reviews previous strategies for a vehicle-based plume tracing. The main contribution is a new source-likelihood mapping approach based on Bayesian inference methods. Using this Bayesian methodology, the source-likelihood map is propagated through time and updated in response to both detection and nondetection events. Examples are included that use data from in-water testing to compare the mapping approach derived herein with the map derived using a previously existing technique.},
author = {Pang, Shuo and Farrell, Jay a},
issn = {1083-4419},
journal = {IEEE transactions on systems, man, and cybernetics. Part B, Cybernetics : a publication of the IEEE Systems, Man, and Cybernetics Society},
keywords = {Air Pollution,Air Pollution: analysis,Algorithms,Artificial Intelligence,Bayes Theorem,Computer Simulation,Diffusion,Environmental Monitoring,Environmental Monitoring: methods,Microfluidics,Microfluidics: methods,Models, Chemical,Models, Statistical,Pattern Recognition, Automated,Pattern Recognition, Automated: methods,Principal Component Analysis,Robotics,Robotics: methods,Smell,Water Pollutants, Chemical,Water Pollutants, Chemical: analysis},
month = oct,
number = {5},
pages = {1068--80},
pmid = {17036813},

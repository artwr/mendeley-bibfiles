@article{Kashparov2005,
abstract = {Long-term field experiments have been carried out in the Chernobyl exclusion zone in order to determine the parameters governing radiochlorine (36Cl) transfer to plants from four types of soil, namely, podzoluvisol, greyzem, and typical and meadow chernozem. Radiochlorine concentration ratios (CR) in radish roots (15+/-10), lettuce leaves (30+/-15), bean pods (15+/-11) and wheat seed (23+/-11) and straw (210+/-110) for fresh weight of plants were obtained. These values correlate well with stable chlorine values for the same plants. One year after injection, 36Cl reached a quasi-equilibrium with stable chlorine in the agricultural soils and its behavior in the soil-plant system mimicked the behavior of stable chlorine (this behavior was determined by soil moisture transport in the investigated soils). In the absence of intensive vertical migration, more than half of 36Cl activity in arable layer of soil passes into the radish, lettuce and the aboveground parts of wheat during a single vegetation period.},
author = {Kashparov, V and Colle, C and Zvarich, S and Yoschenko, V and Levchuk, S and Lundin, S},
doi = {10.1016/j.jenvrad.2004.07.001},

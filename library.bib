@inproceedings{Heron2004,
abstract = {In May and June 2002, the U.S. Air Force and AFCEE conducted the first application of steam-enhanced remediation in a fractured rock aquifer in the US, at Edwards AFB, Site 61. The site geology consists of a thin layer of alluvium resting on 30 to 40 ft of weathered granitic bedrock, which grades downwards into unweathered frac- tured granitic bedrock with enclaves of weathered diorite. The water table is located at a depth of about 32ft. Groundwater flow is towards the east-southeast, leading to the crea- tion of an elongate plume of chlorinated solvents, principally TCE. Dissolved-phase TCE contamination was present to depths of at least 100 ft, with high levels indicating poten- tial DNAPL presence mainly in the upper 60 ft. The treatability study was designed to evaluate the applicability of steam-enhanced remediation for restoration of fractured rock source zones and aquifers impacted by volatile organic compounds. Steam was injected in three discrete intervals within one nested well to a target depth range from 12 to 60 ft bgs. Contaminant vapors and liquids were extracted from four surrounding wells. The upper 40 ft of the treatment zone was heated within 20 to 30 days of initiation of steam injection. The initial high-vacuum extraction resulted in high concentrations of TCE and other VOCs in the extracted vapor. After 30 days of heating, pressure cycling was initiated, resulting in increased mass removal. After heating one of the extraction wells to over 140°F, LNAPL was recovered in a liquid form. The total mass of contaminants removed from the subsurface was approximately 2,000 to 3,000 pounds, including 250 pounds of LNAPL. After cool-down, soil, rock and water samples were collected to assess the remedial success. Soil and rock concentrations of contaminants were below the detection limit in all samples from the surface to a depth of 32 ft. In the upper part of the saturated area (from 32 to 45 ft below grade), TCE groundwater concentrations were low- ered from 500–2,200 µg/L to 0.8–200 µg/L (approximately 90\% reduction). The results were encouraging: Steam enhanced remediation was recommended as a source zone remediation technology in combination with plume control.},
author = {Heron, Gorm and Carroll, Steven and Crisp, Gregg and Sowers, Hank J and Palmer, Scott and Coleman, Kimberly and Watts, Stephen},
booktitle = {Remediation of Chlorinated and Recalcitrant Compounds},
isbn = {1574771450},
number = {May},
publisher = {Battelle Press},
title = {{Steam in fractured rock: source removal at edwards afb}},
year = {2004}
}

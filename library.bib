issn = {0169-7722},
journal = {Journal of contaminant hydrology},
keywords = {Biodegradation,Biological Transport,Canada,Carbon Isotopes,Carbon Isotopes: analysis,Chemical,Chemical: metabolism,Diffusion,Environmental,Environmental Monitoring,Soil Pollutants,Soil Pollutants: metabolism,Tetrachloroethylene,Tetrachloroethylene: chemistry,Tetrachloroethylene: metabolism,Trichloroethylene,Trichloroethylene: chemistry,Trichloroethylene: metabolism,Water Pollutants},
month = oct,
number = {1-4},
pages = {265--82},
pmid = {15358496},
title = {{Effect of source variability and transport processes on carbon isotope ratios of TCE and PCE in two sandy aquifers.}},

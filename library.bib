issn = {0021-9258},
journal = {The Journal of biological chemistry},
keywords = {Animals,Arabidopsis Proteins,Arabidopsis Proteins: chemistry,Calcium,Calcium: metabolism,Dimerization,Green Fluorescent Proteins,Green Fluorescent Proteins: chemistry,Hydrogen-Ion Concentration,Membrane Potentials,Oocytes,Oocytes: metabolism,Plant Proteins,Plant Proteins: chemistry,Potassium,Potassium Channels,Potassium Channels: chemistry,Potassium: chemistry,Proto-Oncogene Proteins c-akt,Proto-Oncogene Proteins c-akt: chemistry,Tobacco,Tobacco: metabolism,Two-Hybrid System Techniques,Voltage-Gated,Xenopus},
month = jan,

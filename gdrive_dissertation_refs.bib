author = {Carlton, W. H. and Bauer, L. R. and Evans, A. G. and Geary, L. A. and Murphy, C E Jr. and Pinder, J. E. and Strom},
institution = {Westinghouse Savannah river company, Savannah River Site},
keywords = {Caesium,Cesium},
title = {{CESIUM IN THE SAVANNAH RIVER SITE ENVIRONMENT ( u ) Executive Summary}},
year = {1992}
}
@article{Kaplan2011,
abstract = {Field and laboratory studies were carried out to understand the cause for steady increases in (129)I concentrations emanating from radiological basins located on the Savannah River Site, South Carolina. The basins were closed in 1988 by adding limestone and slag and then capping with a low permeability engineered cover. Groundwater (129)I concentrations in a well near the basins in 1993 were 200 pCi L(-1) and are presently between 400 and 1000 pCi L(-1). Iodine speciation in the plume contained wide ranges of iodide, iodate, and organo-iodine concentrations. First-order calculations based on a basin sediment desorption study indicate that the modest increase of 0.7 pH units detected in the study site groundwater over the last 17 years since closure of the basins may be sufficient to produce the observed increased groundwater (129)I concentrations near the basins. Groundwater monitoring of the plume at the basins has shown that the migration of many of the high risk radionuclides originally present at this complex site has been attenuated. However, (129)I continues to leave the source at a rate that may have been exacerbated by the initial remediation efforts. This study underscores the importance of identifying the appropriate in situ stabilization technologies for all source contaminants, especially if their geochemical behaviors differ.},
author = {Kaplan, Daniel I and Roberts, Kimberley A. and Schwehr, Kathy a and Lilley, Michael S. and Brinkmeyer, Robin and Denham, Miles E. and Diprete, David and Li, Hsiu-Ping and Powell, Brian A. and Xu, Chen and Yeager, Chris M. and Zhang, Saijin and Santschi, Peter H},
doi = {10.1021/es103314n},
issn = {1520-5851},
journal = {Environmental science \& technology},
keywords = {Adsorption,Environmental Remediation,Environmental Remediation: methods,Iodine Radioisotopes,Iodine Radioisotopes: analysis,Iodine Radioisotopes: chemistry,Radiation Monitoring,Radiation Monitoring: methods,Rivers,Rivers: chemistry,South Carolina,Water Movements,Water Pollutants, Radioactive,Water Pollutants, Radioactive: analysis,Water Pollutants, Radioactive: chemistry},
month = jan,
number = {2},
pages = {489--95},
pmid = {21138294},
title = {{Evaluation of a radioiodine plume increasing in concentration at the Savannah River Site.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21138294},
volume = {45},
year = {2011}
}

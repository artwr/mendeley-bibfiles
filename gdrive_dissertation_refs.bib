month = apr,
number = {8},
pages = {4490--7},
pmid = {22432961},
title = {{Persistent source influences on the trailing edge of a groundwater plume, and natural attenuation timeframes: the F-Area Savannah River Site.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22432961},
volume = {46},
year = {2012}
}
